{
  "name": "@aibex/botscripten",
  "version": "0.5.0",
  "description": "Craft rich bot conversations using the Twine/Twee format",
  "author": "Aibex, Inc <oss@aibex.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "github:aibexhq/botscript",
  "main": "index.js",
  "browser": "dist/Twine2/BotscriptenViewer/format.js",
  "scripts": {
    "dev": "NODE_ENV=development concurrently -r \"npm:dev:*\"",
    "test": "yarn build && jest",
    "build": "yarn clean:dist && concurrently \"npm:build:*\"",
    "buildAll": "yarn clean:dist && concurrently \"npm:build:*\" && yarn examples",
    "tweego": "TWEEGO_PATH=./dist/Twine2 tweego",
    "ci": "pretty-quick --check && yarn build && jest",
    "release": "release-it",
    "____________________": "echo \"The following sub-commands are used to serve the main commands above\"",
    "build:twine": "rollup -c -m inline && node scripts/createTwine",
    "build:node": "rm -rf dist/node/* && babel src/node --out-dir dist/npm/node && babel src/common --out-dir dist/npm/common",
    "clean:dist": "rm -rf dist/",
    "dev:watch": "nodemon -L --ignore dist/ --exec yarn buildAll",
    "dev:serve": "nodemon -L --watch build/ --watch scripts/ --exec node scripts/serve",
    "examples": "concurrently -r \"npm:examples:*\"",
    "examples:onboarding": "yarn tweego -f BotscriptenViewer -o ./examples/onboarding.html ./examples/onboarding.twee",
    "examples:onboarding.min": "yarn tweego -f Botscripten -o ./examples/onboarding.min.html ./examples/onboarding.twee",
    "format:prettier": "pretty-quick --staged",
    "husky:precommit": "concurrently \"npm:format:*\" && yarn build && git add dist/ && jest"
  },
  "keywords": [
    "twine",
    "twinery",
    "storyformat",
    "trialogue",
    "botscript",
    "bot",
    "botframework"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run husky:precommit"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.7.0",
    "html-entities": "^1.2.1",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "lodash.escape": "^4.0.1",
    "lodash.template": "^4.5.0",
    "lodash.unescape": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "7.7.4",
    "concurrently": "^5.0.0",
    "extwee": "^1.5.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.0",
    "nodemon": "^2.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.0",
    "release-it": "^12.3.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "static-server": "^2.2.1"
  }
}
