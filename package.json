{
  "name": "chatbook",
  "version": "0.1.1",
  "twine": {
    "version": "2",
    "formatVersion": "0.1.1"
  },
  "description": "A modified Twine engine specifically for chat conversations, based on Trialogue",
  "author": "Jakob Heuser <jakob+npm@felocity.com>",
  "license": "Apache-2.0",
  "repository": "github:aibexhq/chatbook",
  "main": "index.js",
  "browser": "dist/chatbook.umd.js",
  "scripts": {
    "dev": "NODE_ENV=development concurrently -r \"npm:dev:*\"",
    "test": "yarn build && jest",
    "build": "yarn clean:dist && concurrently \"npm:build:*\"",
    "buildAll": "yarn clean:dist && concurrently \"npm:build:*\" && yarn examples",
    "tweego": "TWEEGO_PATH=./dist/Twine2 tweego",
    "ci": "pretty-quick --check && yarn build && jest",
    "release": "release-it",
    "____________________": "echo \"The following sub-commands are used to serve the main commands above\"",
    "build:twine": "rollup -c -m inline && node scripts/createTwine && yarn clean:artifacts",
    "build:node": "rm -rf dist/node/* && babel src/node --out-dir dist/npm/node && babel src/common --out-dir dist/npm/common",
    "clean:dist": "rm -rf dist/",
    "clean:artifacts": "rm -rf dist/chatbook.*",
    "dev:watch": "nodemon -L --ignore dist/ --exec yarn buildAll",
    "dev:serve": "nodemon -L --watch build/ --watch scripts/ --exec node scripts/serve",
    "examples": "concurrently -r \"npm:examples:*\"",
    "examples:onboarding": "yarn tweego -f ChatbookViewer -o ./examples/onboarding.html ./examples/onboarding.twee",
    "examples:onboarding.min": "yarn tweego -f Chatbook -o ./examples/onboarding.min.html ./examples/onboarding.twee",
    "format:prettier": "pretty-quick --staged",
    "husky:precommit": "concurrently \"npm:format:*\" && yarn build && jest",
    "prepublishOnly": "echo \"publish is disabled. Did you mean `yarn release`?\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run husky:precommit"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "lodash.escape": "^4.0.1",
    "lodash.template": "^4.5.0",
    "lodash.unescape": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "7.4.5",
    "concurrently": "^4.1.1",
    "extwee": "^1.5.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.0",
    "nodemon": "^1.19.1",
    "prettier": "1.17.0",
    "pretty-quick": "^1.11.1",
    "release-it": "^12.3.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "static-server": "^2.2.1"
  }
}
